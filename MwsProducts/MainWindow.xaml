<Window x:Class="MwsProducts.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MarketplacewebServiceProducts" Height="350" Width="525">
    <Grid>
        <TabControl>
            <TabItem Header="ListMatchingProducts">
                <Grid Background="#FFE5E5E5">
