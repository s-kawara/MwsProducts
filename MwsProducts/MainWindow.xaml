                            Content="検索実行" 
                            Width="120" 
                            Height="25"
                            Margin="10,50,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Click="btnListMatchingSearch_Click"
                            />
                    <TextBlock x:Name="txtListMatchingResult"
                               Width="250"
                               Height="200"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               Margin="200 10 0 0"
                               />
                </Grid>
            </TabItem>
